{
  "name": "jest-1",
  "version": "1.0.0",
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "@types/jest": "^24.0.11",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "^6.26.3",
    "jest": "24",
    "jest-serializer-vue": "2",
    "node-fetch": "^2.3.0",
    "ts-jest": "24",
    "ts-loader": "5",
    "typescript": "3",
    "vue": "2",
    "vue-jest": "3",
    "vue-loader": "15",
    "vue-template-compiler": "2.6.10",
    "webpack": "4",
    "webpack-cli": "3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue"
    ],
    "transform": {
      "^.+\\.(j|t)s$": "ts-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "testMatch": [
      "<rootDir>/src/*.spec.(js|ts)"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "setupFiles": ["<rootDir>/jest-setup.ts"]
  },
  "keywords": [],
  "description": ""
}
